{
	"$schema": "https://biomejs.dev/schemas/1.8.0/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"useImportType": "info",
				"noUnusedTemplateLiteral": "off"
			},
			"suspicious": {
				"noExplicitAny": "warn"
			},
			"correctness": {
				"useExhaustiveDependencies": "warn"
			},
			"complexity": {
				"noForEach": "off"
			}
		}
	}
}