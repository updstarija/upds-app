{
  "name": "my-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --localhost -d",
    "android": "npm run android:sandbox",
    "android:sandbox": "npm run prepare:sandbox && expo run:android --device",
    "android:prerelease": "npm run prepare:prerelease &&  NODE_ENV=prerelease expo run:android --device",
    "android:production": "npm run prepare:production && NODE_ENV=production expo run:android --device",
    "ios": "npm run ios:sandbox",
    "ios:sandbox": "npm run prepare:sandbox && expo run:ios --device",
    "ios:prerelease": "npm run prepare:prerelease && NODE_ENV=prerelease  expo run:ios --device",
    "ios:production": "npm run prepare:production &&  NODE_ENV=production expo run:ios --device",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "build:sandbox": "npm run prepare:sandbox && eas build -p android --profile sandbox --local",
    "build:prerelease": "npm run prepare:prerelease && eas build -p android --profile prerelease --local",
    "build:production": "npm run prepare:production && eas build -p android --local",
    "build:production:apk": "npm run prepare:production && eas build -p android --profile production-apk --local",
    "pre-build": "expo prebuild",
    "prepare:production": "cp -f ./google-services.production.json ./android/app/google-services.json && cp -f ./GoogleService-info.production.plist ./ios/UPDSTarija/GoogleService-info.plist",
    "prepare:prerelease": "cp -f ./google-services.production.json ./android/app/google-services.json && cp -f ./GoogleService-info.production.plist ./ios/UPDSTarija/GoogleService-info.plist",
    "prepare:sandbox": "cp -f ./google-services.sandbox.json ./android/app/google-services.json && cp -f ./GoogleService-info.sandbox.plist ./ios/UPDSTarija/GoogleService-info.plist"
  },
  "jest": {
    "preset": "jest-expo"
  },
